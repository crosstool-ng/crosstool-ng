# GNU make

## help GNU make

choice
    bool
    prompt "make version"
# Don't remove next line
# CT_INSERT_VERSION_BELOW

config MAKE_V_4_2_1
    bool
    prompt "4.2.1"

config MAKE_V_4_1
    bool
    prompt "4.1"

config MAKE_V_4_0
    bool
    prompt "4.0"

config MAKE_V_3_81
    bool
    prompt "3.81"

endchoice

config MAKE_GMAKE_SYMLINK
    bool
    prompt "Add gmake symlink to companion gnu/make"
    depends on COMP_TOOLS_make

config MAKE_VERSION
    string
# Don't remove next line
# CT_INSERT_VERSION_STRING_BELOW
    default "4.2.1" if MAKE_V_4_2_1
    default "4.1" if MAKE_V_4_1
    default "4.0" if MAKE_V_4_0
    default "3.81" if MAKE_V_3_81
