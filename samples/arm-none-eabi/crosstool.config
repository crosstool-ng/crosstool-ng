CT_DEBUG_CT=y
CT_DEBUG_CT_SAVE_STEPS=y
CT_LOCAL_TARBALLS_DIR="${HOME}/src"
CT_SAVE_TARBALLS=y
# CT_INSTALL_DIR_RO is not set
CT_LOG_EXTRA=y
CT_ARCH_arm=y
CT_MULTILIB=y
# CT_ARCH_USE_MMU is not set
CT_TOOLCHAIN_PKGVERSION="GNU Tools for ARM Embedded Processors"
CT_TARGET_VENDOR="none"
CT_BINUTILS_PLUGINS=y
CT_BINUTILS_EXTRA_CONFIG_ARRAY="--enable-interwork"
CT_LIBC_NEWLIB_TARGET_CFLAGS="-ffunction-sections -fdata-sections"
CT_LIBC_NEWLIB_IO_LL=y
CT_LIBC_NEWLIB_DISABLE_SUPPLIED_SYSCALLS=y
# CT_LIBC_NEWLIB_ENABLE_TARGET_OPTSPACE is not set
CT_LIBC_NEWLIB_REGISTER_FINI=y
CT_LIBC_NEWLIB_EXTRA_CONFIG_ARRAY="--disable-nls"
CT_CC_GCC_CORE_EXTRA_CONFIG_ARRAY="--with-multilib-list=armv6-m,armv7-m,armv7e-m,armv7-r --disable-tls"
CT_CC_GCC_EXTRA_CONFIG_ARRAY="--enable-plugin --with-multilib-list=armv6-m,armv7-m,armv7e-m,armv7-r --disable-tls"
CT_CC_GCC_TARGET_FINAL=y
# CT_CC_GCC_ENABLE_TARGET_OPTSPACE is not set
CT_CC_GCC_DISABLE_PCH=y
# CT_CC_GCC_LDBL_128 is not set
CT_CC_LANG_CXX=y
CT_DEBUG_gdb=y
CT_GDB_CROSS_EXTRA_CONFIG_ARRAY="--with-lzma=no"
